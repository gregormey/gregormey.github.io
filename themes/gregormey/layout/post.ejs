<div class="post">
	<div class="post__back">
    	<a href="/">&lt;-- home</a>
    </div>
    <div class="post__title">
    	<h1><%= page.title %></h1>
    </div>
    <div class="post__date">
    	<p><%= date(page.date) %></p>
    </div>
    <div class="post__meta">
    	<% page.tags.each(function(tag){ %>
          <p><%= tag %></p>
        <% }); %>
    </div>
    <div class="post__content">
        <%- page.content %>
    </div>
</div>
<%- partial('_partial/disqus',{page_url:url, page_identifier:page.path}) %>
